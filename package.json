{
    "name": "multi-rename",
    "displayName": "Multi Rename",
    "publisher": "zardoy",
    "version": "0.0.0-dev",
    "license": "MIT",
    "web": true,
    "categories": [
        "Other",
        "Programming Languages"
    ],
    "contributes": {
        "commands": [
            {
                "command": "startRename",
                "title": "Start Rename"
            },
            {
                "command": "processRename",
                "title": "Process Rename"
            }
        ],
        "menus": {
            "editor/title": [
                {
                    "command": "processRename",
                    "group": "navigation",
                    "when": "resourceScheme == multiRename"
                }
            ]
        }
    },
    "activationEvents": [
        "onCommands"
    ],
    "scripts": {
        "start": "vscode-framework start --skip-launching",
        "build": "vscode-framework build"
    },
    "devDependencies": {
        "@types/lodash": "^4.14.191",
        "@types/vscode": "1.71.0",
        "@zardoy/tsconfig": "^1.5.0",
        "@zardoy/vscode-utils": "^0.0.47",
        "typescript": "^4.9.5",
        "vscode-framework": "^0.0.18"
    },
    "prettier": {
        "semi": false,
        "singleQuote": true,
        "proseWrap": "never",
        "tabWidth": 4,
        "trailingComma": "all",
        "arrowParens": "avoid",
        "printWidth": 160,
        "endOfLine": "auto"
    },
    "dependencies": {
        "lodash": "^4.17.21"
    }
}
